package myFirstProject;
import java.util.Arrays;


public class Final {
	final static int a=5;
	public static void main(String[] args) {
		//a = 10; final cannot be assigned
		asubclass sub = new asubclass();
		sub.show();//Can be called
		final int[] b = {1, 2, 3};
		System.out.println("before:"+System.identityHashCode(b));
		for(int i=0; i<3; i++) {
			b[i] = b[i]+i;
		}
		System.out.println(Arrays.toString(b));
		System.out.println("after:"+System.identityHashCode(b));
		//The content of the final object can be modified, but the address remains the same
	}
}

final class afinal{
	final void show() {
		System.out.println("afinal.show()");
	}
}

//final class cannot be extended
/*
class asubclass extends afinal{
	
}
*/

class aclass{
	final void show() {
		System.out.println("aclass.show()");
	}
}

class asubclass extends aclass{
	//The final method can be inherited, but cannot be overridden
	//void show() {
	//	System.out.println("asubclass.show()");
	//}
}


