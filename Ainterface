package myFirstProject;

public class Ainterface implements SpeakChinese,SpeakEnglish,SpeakChineseEnglish{
	public void ashow(){
		System.out.println("你好！");
	}
	public void bshow(){
		System.out.println("Hello！");
	}
	public void cshow(){
		System.out.println("Hello！"+"你好！");
	}

	public static void main(String[] args) {
		Ainterface man = new Ainterface();
		man.ashow();
		man.bshow();
		man.cshow();
	}
	
}

interface SpeakChinese{
	void ashow();
}

interface SpeakEnglish{
	void bshow();
}

interface SpeakChineseEnglish{
	void cshow();
}
