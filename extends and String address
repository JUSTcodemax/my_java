	A(){
		x=x+1;
		showX();
	}
	
	public void showX() {
		System.out.println("A.x="+x);
	}
	
	public int getY() {
		return y;
	}
	
}

class B extends A{
	B(int x){
		x=x+2;
		showX();
	}
	
	public void showX() {
		System.out.println("B.x="+x);
	}
	
	public int getY() {
		System.out.println("B.y="+(super.getY()+x));
