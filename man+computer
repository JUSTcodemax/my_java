import java.io.*;


public class T1 {
	public static void main(String[] args){
		Man man = new Man();
		Woman woman = new Woman();
		NoteBookPC notebook = new NoteBookPC();
		PadPC pad = new PadPC();
		man.OperateWay1();
		woman.OperateWay2();
		notebook.ShowInfo2();
		pad.ShowInfo1();
	}

}

abstract class Human implements OperateWay{
	//public void OperateWay1() {
	//}
	//public void OperateWay2() {
	//}
}

class Man extends Human{
	public void OperateWay1() {
		System.out.print("[Man]");
		OperateWay1.OperateWay1();
	}
	public void OperateWay2() {
		System.out.print("[Man]");
		OperateWay2.OperateWay2();
	}
}

class Woman extends Human{
	public void OperateWay1() {
		System.out.print("[Woman]");
		OperateWay1.OperateWay1();
	}
	public void OperateWay2() {
		System.out.print("[Woman]");
		OperateWay2.OperateWay2();
	}
}

abstract class Computer implements Show{
	//public void ShowInfo1(){
	//}
	//public void ShowInfo2(){
	//}
}

class NoteBookPC extends Computer{
	public void ShowInfo1(){
		System.out.print("[NoteBookPC]");
		ShowInfo1.ShowInfo1();
	}
	
	public void ShowInfo2(){
		System.out.print("[NoteBookPC]");
		ShowInfo2.ShowInfo2();
	}
}

class PadPC extends Computer{
	public void ShowInfo1(){
		System.out.print("[PadPC]");
		ShowInfo1.ShowInfo1();
	}
	
	public void ShowInfo2(){
		System.out.print("[PadPC]");
		ShowInfo2.ShowInfo2();
	}
}

interface OperateWay{
	void OperateWay1();
	void OperateWay2();
	
}

interface Show{
	void ShowInfo1();
	void ShowInfo2();
	
}

class OperateWay1{
	public static void OperateWay1() {
		System.out.println("触摸屏操作");
	}

}
class OperateWay2{
	public static void OperateWay2() {
		System.out.println("键盘鼠标操作");
	}
}

class ShowInfo1{
	public static void ShowInfo1(){
		System.out.println("你好！");
	}
}
class ShowInfo2{
	public static void ShowInfo2(){
		System.out.println("Hello!");
	}
}
